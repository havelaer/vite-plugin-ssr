{
  "name": "@havelaer/vite-plugin-ssr",
  "version": "0.0.9",
  "description": "SSR plugin for Vite. With optional API servers.",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/havelaer/vite-plugin-ssr.git"
  },
  "exports": {
    ".": {
      "import": "./dist/plugin.js",
      "types": "./dist/plugin.d.ts"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "dev": "tsdown src/plugin.ts --dts --watch ./src",
    "check": "biome check .",
    "release": "npm run check && tsc && npm version patch -m 'chore: bump version %s'",
    "postrelease": "git push && git push --tags",
    "build": "tsdown src/plugin.ts --dts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "vite",
    "ssr"
  ],
  "author": "Havelaer",
  "license": "MIT",
  "peerDependencies": {
    "vite": "^6.0.0 || ^7.0.0"
  },
  "workspaces": [
    ".",
    "examples/*"
  ],
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@types/node": "^24.1.0",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.3",
    "vite": "^7.1.7"
  },
  "dependencies": {
    "@hono/node-server": "^1.18.0",
    "cheerio": "^1.1.2"
  }
}
