{
  "name": "@havelaer/vite-plugin-ssr",
  "version": "0.0.2",
  "description": "SSR for Vite",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/plugin.js",
      "types": "./dist/plugin.d.ts"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsdown src/plugin.ts --dts",
    "dev": "tsdown src/plugin.ts --dts --watch",
    "lint": "biome check .",
    "prepublishOnly": "npm run lint -- --fix && npm run build && npm version patch -m 'chore: publishing version %s'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "vite",
    "ssr"
  ],
  "author": "Havelaer",
  "license": "MIT",
  "peerDependencies": {
    "vite": "^6.0.0 || ^7.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@types/node": "^24.1.0",
    "tsdown": "^0.13.1",
    "typescript": "^5.9.2",
    "vite": "^7.0.6"
  },
  "dependencies": {
    "@hono/node-server": "^1.18.0",
    "cheerio": "^1.1.2"
  }
}
